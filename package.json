{
  "name": "rollup-plugin-ractive",
  "description": "Compile Ractive components",
  "version": "1.2.1",
  "author": "Rich Harris",
  "repository": "https://github.com/rollup/rollup-plugin-ractive",
  "license": "MIT",
  "main": "dist/rollup-plugin-ractive.cjs.js",
  "jsnext:main": "dist/rollup-plugin-ractive.es6.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:es6",
    "build:cjs": "rollup -c -f cjs -o dist/rollup-plugin-ractive.cjs.js",
    "build:es6": "rollup -c -f es6 -o dist/rollup-plugin-ractive.es6.js",
    "test": "mocha --compilers js:buble/register test/test.js",
    "pretest": "npm run build:cjs",
    "prepublish": "npm run lint && rm -rf dist && npm test && npm run build:es6",
    "lint": "eslint src"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "buble": "^0.5.3",
    "eslint": "^2.8.0",
    "mocha": "^2.4.5",
    "ractive": "^0.7.3",
    "rollup": "^0.25.2",
    "rollup-plugin-buble": "^0.5.0"
  },
  "dependencies": {
    "rcu": "^0.8.1",
    "rcu-builders": "^0.5.1",
    "rollup-pluginutils": "^1.3.1"
  }
}
